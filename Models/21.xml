  <!-- Intérieur -->
  <model>
    <path>Aircraft/21/Models/Interior/interior.xml</path>
  </model>
  
  <!-- Une ombre pour OSG -->
  <model>
    <path>Aircraft/21/Models/shadow.xml</path>
    <offsets>
      <x-m>  0.0 </x-m>
      <y-m>  0.0 </y-m>
      <z-m> -1.1 </z-m>
    </offsets>
  </model>

  <!-- The engine -->  
  <model>
    <path>Aircraft/21/Models/Engine/engine.xml</path>
    <offsets>
      <x-m> -3.60025 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m>  0.34184 </z-m>
    </offsets>
  </model>
  
  <!-- Cranking effects -->
  <model>
    <path>Aircraft/21/Models/Effects/cranking/cranking.xml</path>
    <offsets>
      <x-m> -3.60025 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m>  0.34184 </z-m>
    </offsets>
  </model>
  
  <!-- Effets sur le sol -->
  <!-- Left wheel -->
  <model>
    <path>Aircraft/21/Models/Effects/ground/ground0.xml</path>
    <offsets>
      <x-m> -3.367 </x-m>
      <y-m> -0.376 </y-m>
      <z-m> -1.193 </z-m>
    </offsets>
  </model>
  
  <!-- Right wheel -->
  <model>
    <path>Aircraft/21/Models/Effects/ground/ground1.xml</path>
    <offsets>
      <x-m> -3.367 </x-m>
      <y-m>  0.376 </y-m>
      <z-m> -1.193 </z-m>
    </offsets>
  </model>
  
  <!-- Rear -->
  <model>
    <path>Aircraft/21/Models/Effects/ground/ground2.xml</path>
    <offsets>
      <x-m>  0.867 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -1.158 </z-m>
    </offsets>
  </model>
  
  <!-- L'hélice --> 
  <animation>
    <name>HeliceComplete</name>
    <object-name>propdisc</object-name>
    <object-name>propblur</object-name>
    <object-name>helice</object-name>
  </animation>
  
  <!-- Séléction de l'objet visible en fonction des rpm -->
  <animation>
    <type>select</type>
    <object-name>propdisc</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>1500</value>
      </greater-than>
    </condition>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>propblur</object-name>
    <condition>
      <and>
        <greater-than>
          <property>engines/engine[0]/rpm</property>
          <value>600.000</value>
        </greater-than>
        <less-than>
          <property>engines/engine[0]/rpm</property>
          <value>1500.001</value>
        </less-than>
      </and>
    </condition>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>helice</object-name>
    <condition>
      <less-than>
        <property>engines/engine[0]/rpm</property>
        <value>1500</value>
      </less-than>
    </condition>
  </animation>
  
  <animation>
    <type>spin</type>
    <object-name>HeliceComplete</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value> 5 </value>
      </greater-than>
    </condition>
    <property>engines/engine[0]/rpm</property>
    <factor> -1 </factor>
    <center>
      <x-m> -3.821 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.325 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <!-- Gouverne de profondeur -->
  <animation>
    <name>DeriveComplete</name>
    <object-name>queue</object-name>
    <object-name>queuesoie</object-name>
    <object-name>prof1</object-name>
    <object-name>prof3</object-name>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>DeriveComplete</object-name>
    <property>surface-positions/elevator-pos-norm</property>
    <factor> 15 </factor>
    <center>
      <x-m>  2.857</x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.323 </z-m>
    </center>
    <axis>
      <x> 0.000 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
